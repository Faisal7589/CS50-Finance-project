CREATE TABLE users (id INTEGER, username TEXT NOT NULL, hash TEXT NOT NULL, cash NUMERIC NOT NULL DEFAULT 10000.00, SERIAL PRIMARY KEY(id));
CREATE UNIQUE INDEX username ON users (username);
CREATE TABLE IF NOT EXISTS "transactions" (id INTEGER, symbol TEXT NOT NULL, shares INTEGER NOT NULL DEFAULT 0,price INTEGER NOT NULL, total INTEGER NOT NULL,"time" TIMESTAMP, user_id INTEGER, SERIAL PRIMARY KEY(id), FOREIGN KEY(user_id) REFEREnCES users(id));